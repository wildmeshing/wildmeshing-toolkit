{
    "input": {
        "path": "unit_test/meshes/camel.msh",
        "name_spec": "pos"
    },
        "position_attribute": "pos/vertices",
        "iterations": 60,
        "length_rel": 0.01,
        "envelope_size": 1e-2,
        "lock_boundary": true,
        "edge_swap_mode": "valence",
        "use_for_periodic": false,
        "fix_uv_seam": true,
        "output": {
            "path": "camel.hdf5",
            "type": ".hdf5"
        },
        "intermediate_output_format": {
            "pos": {
                "path": "camel_{:04}",
                "type": ".vtu",
                "position_attribute": "vertices"
            }
        }
}
