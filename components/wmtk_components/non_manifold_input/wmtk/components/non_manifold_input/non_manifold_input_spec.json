[
  {
    "pointer": "/",
    "type": "object",
    "required": ["name", "file"],
    "optional": [
      "ignore_z",
      "tetrahedron_attributes",
      "non_manifold_vertex_label",
      "non_manifold_edge_label",
      "non_manifold_tag_value"
    ]
  },
  {
    "pointer": "/name",
    "type": "string"
  },
  {
    "pointer": "/file",
    "type": "string"
  },
  {
    "pointer": "/ignore_z",
    "type": "bool",
    "default": false
  },
  {
    "pointer": "/tetrahedron_attributes",
    "type": "list",
    "default": [],
    "doc": "all attributes on tetrahedra that should be loaded from the msh file"
  },
  {
    "pointer": "/tetrahedron_attributes/*",
    "type": "string"
  },
  {
    "pointer": "/non_manifold_vertex_label",
    "type": "string",
    "default": "vertex_non_manifold_tag"
  },
  {
    "pointer": "/non_manifold_edge_label",
    "type": "string",
    "default": "edge_non_manifold_tag"
  },
  {
    "pointer": "/non_manifold_tag_value",
    "type": "int",
    "default": "1"
  }
]
